switch (mynodetype) {
	case NODETYPE.none: mycolour = c_white; break;
	case NODETYPE.attack: mycolour = c_red; break;
	case NODETYPE.heal: mycolour = c_green; break;
	case NODETYPE.comboup: mycolour = c_yellow; break;
	case NODETYPE.decaystop: mycolour = c_purple; break;
	case NODETYPE.defense: mycolour = c_blue;
}


